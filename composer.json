{
    "name": "adamtrll/nova-image-markup-field",
    "description": "A Laravel Nova field for adding simple markup to images.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Adamtrll\\NovaImageMarkupField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adamtrll\\NovaImageMarkupField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
